			description="Groupe de tous les utilisateurs pouvant accéder à Change. Ce groupe ne peut pas être supprimé.">
			<user email="admin-webfactory@rbs.fr" isroot="true"
				login="wwwadmin"  passwordmd5="326379df03facd04daff1578132d2540"
				publicationstatus="ACTIVE">
				<usersprofile firstname="Administrateur" lastname="Change" titleid-refid="mr" />
			</user>
		</backendgroup>
	</rootfolder>
		
	<systemfolder module="task" relatedmodule="users">
		<plannedtask label="Rafraichissement des groupes dynamiques" systemtaskclassname="users_RefreshDynamicgroupsTask" minute="0" hour="2" />
	</systemfolder>

