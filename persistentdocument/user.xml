<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.rbs.fr/schema/change-document/1.0 http://www.rbschange.fr/schema/persistentdocument-4.0.xsd" model-version="4.0" icon="user-frontoffice" has-url="true">
	<properties>
		<property name="label">
			<constraint name="pseudonym" />
		</property>
		<property name="email" type="String" required="true">
			<constraint name="email" />
		</property>
		<property name="isroot" type="Boolean" default-value="false" />
		<property name="issudoer" type="Boolean" default-value="false" />
		<property name="login" type="String" required="true">
			<constraint name="login" />
		</property>
		<property name="passwordmd5" type="String" indexed="none" db-size="50" required="true" />
		<property name="groups" type="DocumentArray" inverse="true" document-type="modules_users/group" />
		<property name="lastlogin" type="DateTime" />
		<property name="lastping" type="DateTime" />
		<property name="changepasswordkey" type="String" db-size="50" indexed="none" />
	</properties>
</document>